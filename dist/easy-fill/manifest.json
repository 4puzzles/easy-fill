  "name": "Easy Fill",

  "version": "2.0.0",

  "version_name": "2.0.0-alpha.3",
  
  "action": {
    "default_icon": "img/icon.png",
    "default_title": "__MSG_pluginDesc__"
  },

  "default_locale": "zh_CN",

